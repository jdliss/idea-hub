<div class="container">
  <h1>Categories</h1>
  <div class="container">
    <% @categories.each do |category| %>
      <div class="chip">
        <img src="<%= category.image %>" alt="<%= category.name %>" />
        <%= link_to "#{category.name}", category_path(category) %>
      </div><br><br>
    <% end %>
  </div>
</div>
